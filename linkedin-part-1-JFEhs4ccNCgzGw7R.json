    {
      "parameters": {
        "jsCode": "// Check if the input array is not empty and the first item is a valid object.\nif (items && items.length > 0 && items[0] && items[0].json) {\n  // Get the first item from the input.\n  const item = items[0].json;\n\n  // Check if the title property exists and is a valid JSON string.\n  if (item.title) {\n    try {\n      // Parse the 'title' property, which contains a JSON string.\n      // The content is then nested under the 'json' key in the parsed object.\n      const contentJson = JSON.parse(item.title).json;\n\n      // Extract the necessary fields from the parsed JSON.\n      const postTitle = contentJson.title;\n      const postContent = contentJson.content;\n      const postLink = contentJson.link;\n      const postCategories = contentJson.categories;\n\n      // Build the LinkedIn post using emojis and proper formatting.\n      // Start with a rocket emoji and the title in bold.\n      let linkedinPost = `ðŸš€ **${postTitle}**\\n\\n`;\n\n      // Add the main content of the post.\n      linkedinPost += `${postContent}\\n\\n`;\n\n      // Add the call to action and link.\n      linkedinPost += `Learn more at: ${postLink}\\n\\n`;\n\n      // Generate hashtags from the categories by removing spaces and adding a '#'.\n      const hashtags = postCategories.map(tag => `#${tag.replace(/\\s/g, '')}`).join(' ');\n      linkedinPost += hashtags;\n\n      // Add a unique timestamp to the end of the post to prevent the \"Duplicate post is detected\" error.\n      const uniqueIdentifier = `\\n\\n[${Date.now()}]`;\n      linkedinPost += uniqueIdentifier;\n\n      // Create the output item with the final formatted post.\n      return [{ json: { post: linkedinPost } }];\n    } catch (error) {\n      // Handle the case where JSON parsing fails, which is common if the data is not in the expected format.\n      // Log the error to help with debugging and return an empty array to prevent the workflow from failing.\n      console.error('Error parsing JSON from item.title:', error.message);\n      return [];\n    }\n  } else {\n    // If the title property is missing, log a warning and return an empty array.\n    console.warn('The \"title\" property is missing from the input item.');\n    return [];\n  }\n} else {\n  // If no valid items are found, return an empty array to prevent the error.\n  console.warn('No valid items found in the input array. Returning an empty array.');\n  return [];\n}\n"
      },
