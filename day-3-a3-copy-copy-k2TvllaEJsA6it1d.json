    {
      "parameters": {
        "jsCode": "return items.map(item => {\n  // Clone object without \"output\"\n  const { output, ...rest } = item.json;\n\n  const text = rest.cleanedDescription || \"\";\n\n  // Regex for email and numbers\n  const emailRegex = /[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\\.[A-Za-z]{2,}/g;\n  const numberRegex = /\\b\\d{7,15}\\b/g; // captures phone-like numbers\n\n  const emails = text.match(emailRegex) || [];\n  const numbers = text.match(numberRegex) || [];\n\n  return {\n    json: {\n      ...rest,\n      emails,\n      numbers\n    }\n  };\n});\n"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        768,
        256
      ],
      "id": "3e198b19-04e0-465e-9e02-70158b685ab1",
      "name": "Code"
    },
    {
      "parameters": {
        "jsCode": "return items.map(item => {\n  const regexFlag = item.json.regexFlag || false;\n  const aiFlag = item.json.aiFlag || false;\n\n  // Assign scores\n  let score = 0;\n  if (regexFlag) score += 30;\n  if (aiFlag) score += 50;\n\n  // Determine risk level\n  let riskLevel = \"âœ… Pass\";\n  if (score > 40 && score <= 70) riskLevel = \"âš ï¸ Review Needed\";\n  if (score > 70) riskLevel = \"ðŸš¨ High-Risk\";\n\n  return {\n    json: {\n      ...item.json,\n      biasScore: score,\n      riskLevel: riskLevel\n    }\n  };\n});\n"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        1280,
        256
      ],
      "id": "6f73082a-b271-431f-b6b3-dc50022c924b",
      "name": "riskLevel code"
    },
    {
      "parameters": {
        "jsCode": "return items.map(item => {\n  const aiOutput = item.json.output; // the JSON string from AI Agent\n  let flagged = false;\n  let categories = [];\n\n  try {\n    const parsed = JSON.parse(aiOutput);\n    flagged = parsed.flagged || false;\n    categories = parsed.categories || [];\n  } catch (err) {\n    flagged = false;\n    categories = [];\n  }\n\n  // add to item for next step (Bias Scorer)\n  return { \n    json: { \n      ...item.json, \n      aiFlag: flagged, \n      aiCategories: categories \n    } \n  };\n});\n"
      },
